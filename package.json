{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "dev": "npm-run-all --silent --parallel dev:*",
    "dev:client": "webpack-dev-server",
    "dev:server": "nodemon server/index.js",
    "db:import": "source .env && psql $DATABASE_URL < database/dump.sql",
    "db:export": "source .env && pg_dump -c --if-exists -O $DATABASE_URL > database/dump.sql",
    "node": "NODE_ENV=production node server/index.js",
    "start": "node server/index.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "8.2.0",
    "express-session": "1.17.0",
    "gatsby": "^2.26.1",
    "gatsby-plugin-nodejs": "^0.7.0",
    "gatsby-plugin-offline": "^3.10.0",
    "mic-recorder-to-mp3": "^2.2.2",
    "mongodb": "^3.6.4",
    "mongoose": "^5.11.15",
    "pg": "7.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-voice-recorder": "^2.0.7",
    "session-file-store": "1.3.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.7",
    "prettier": "2.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "resolutions": {
    "@aws-amplify/analytics": "1.0.9",
    "@aws-amplify/core": "1.0.9"
  }
}
