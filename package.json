{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "dev": "npm-run-all --silent --parallel dev:*",
    "dev:client": "webpack-dev-server",
    "dev:server": "nodemon server/index.js",
    "db:import": "source .env && psql $DATABASE_URL < database/dump.sql",
    "db:export": "source .env && pg_dump -c --if-exists -O $DATABASE_URL > database/dump.sql",
    "node": "NODE_ENV=production node server/index.js",
    "start": "node server/index.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@react-icons/all-files": "^4.1.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.6.0",
    "dotenv": "8.2.0",
    "express-session": "1.17.0",
    "gatsby": "^3.1.0-next.3",
    "gatsby-plugin-nodejs": "^0.7.0",
    "gatsby-plugin-offline": "^3.10.2",
    "gatsby-plugin-transition-link": "^1.20.5",
    "gsap": "^3.6.0",
    "mic-recorder-to-mp3": "^2.2.2",
    "mongodb": "^3.6.4",
    "mongoose": "^5.11.18",
    "pg": "7.17.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-download-link": "^2.3.0",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "react-voice-recorder": "^2.0.7",
    "session-file-store": "1.3.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "gatsby-plugin-postcss": "^4.0.0",
    "nodemon": "^2.0.7",
    "postcss": "^8.2.7",
    "prettier": "2.2.1",
    "tailwind-styled-components": "^1.0.7",
    "tailwindcss": "^2.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "resolutions": {
    "@aws-amplify/analytics": "1.0.9",
    "@aws-amplify/core": "1.0.9"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env"
          ]
        }
      ]
    ]
  }
}
